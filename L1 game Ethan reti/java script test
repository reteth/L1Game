import java.awt.Image;

public class Sprite {
  private Animation a;
  private float x;
  private float y;
  private float vx;
  private float vy;
  
  
  public Sprite (Animation a ) {
     this.a = a;
  }
  
  public void update(long timePassed ){
x += vx * timepassed;
y += yx * timepassed;
a.update(timepassed);
  }
}